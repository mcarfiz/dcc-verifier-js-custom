                                    {
                                        "plusTime": [
                                            {
                                                "var": "payload.r.0.du"
                                            },
                                            0,
                                            "day"
                                        ]
                                    },
                                    {
                                        "plusTime": [
                                            {
                                                "var": "payload.r.0.fr"
                                            },
